{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["./src/layouts/*"],
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content/*"],
      "@type/*": ["src/types/*"],
      "@image/*": ["src/images/*"],
    },
  },
}
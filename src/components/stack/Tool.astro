---
import { Card} from '../../components/Card'
interface Props {
  title: string;
  href?: string;
}

const {title, href} = Astro.props
---
<Card as="li">
  <Card.Title as="h3" href={href} target="_blank" hasOverallLink={false}>
    {title}
  </Card.Title>
  <Card.Description>
    <slot />
  </Card.Description>
</Card>
